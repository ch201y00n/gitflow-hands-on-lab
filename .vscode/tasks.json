{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "dotnet build",
      "type": "process",
      "command": "dotnet",
      "args": ["build", "${workspaceFolder}"],
      "problemMatcher": "$msCompile",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "dotnet run",
      "type": "process",
      "command": "dotnet",
      "args": ["run", "--project", "${workspaceFolder}"],
      "problemMatcher": "$msCompile",
      "group": {
        "kind": "test",
        "isDefault": false
      }
    }
  ]
}
